<style>
/* CSS Variables */
:root {
  --partner-primary: #009a61;
  --partner-primary-hover: #00824f;
  --partner-accent: #00d97e;
  --partner-text: #1a1a1a;
  --partner-text-secondary: #666666;
  --partner-bg: #ffffff;
  --partner-card-bg: #ffffff;
  --partner-border: rgba(0, 0, 0, 0.08);
  --partner-shadow-sm: 0 4px 12px rgba(0, 0, 0, 0.06);
  --partner-shadow-md: 0 8px 24px rgba(0, 0, 0, 0.08);
  --partner-shadow-lg: 0 12px 32px rgba(0, 154, 97, 0.15);
  --partner-transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
}

/* Dark Mode */
[data-theme="dark"] {
  --partner-text: #f5f5f5;
  --partner-text-secondary: #999999;
  --partner-bg: #0a0a0a;
  --partner-card-bg: rgba(26, 26, 26, 0.8);
  --partner-border: rgba(255, 255, 255, 0.08);
  --partner-primary: #00d97e;
  --partner-accent: #00ff88;
  --partner-shadow-sm: 0 4px 12px rgba(0, 0, 0, 0.3);
  --partner-shadow-md: 0 8px 24px rgba(0, 0, 0, 0.4);
  --partner-shadow-lg: 0 12px 32px rgba(0, 217, 126, 0.2);
}

/* Kill Shopify Grid */
.shopify-section:has(.partner-program-section) {
  display: block !important;
  padding: 0 !important;
  margin: 0 !important;
  grid-template-columns: unset !important;
}

/* Main Section */
.partner-program-section {
  width: 100%;
  background: var(--partner-bg);
  position: relative;
  overflow: hidden;
}

[data-theme="dark"] .partner-program-section {
  background: #0a0a0a;
}

/* Hero Section */
.partner-hero {
  padding: 120px 0 80px 0;
  background: linear-gradient(135deg, #f0faf7 0%, #ffffff 100%);
  position: relative;
  overflow: hidden;
}

[data-theme="dark"] .partner-hero {
  background: linear-gradient(135deg, rgba(0, 217, 126, 0.05) 0%, transparent 100%);
}

.partner-hero::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: radial-gradient(circle at 70% 50%, rgba(0, 154, 97, 0.08) 0%, transparent 50%);
  pointer-events: none;
}

.partner-container {
  max-width: 1200px;
  margin: 0 auto;
  padding: 0 40px;
  position: relative;
  z-index: 1;
}

.partner-hero-content {
  text-align: center;
  max-width: 800px;
  margin: 0 auto;
  animation: fadeInUp 0.8s ease;
}

.partner-label {
  display: inline-block;
  padding: 8px 20px;
  background: var(--partner-primary);
  color: white;
  border-radius: 100px;
  font-size: 14px;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 0.5px;
  margin-bottom: 24px;
}

[data-theme="dark"] .partner-label {
  background: var(--partner-primary);
  color: #0a0a0a;
}

.partner-hero-title {
  font-size: clamp(36px, 6vw, 64px);
  font-weight: 800;
  color: var(--partner-text);
  margin: 0 0 24px 0;
  letter-spacing: -0.02em;
  line-height: 1.1;
}

.partner-hero-highlight {
  background: linear-gradient(135deg, var(--partner-primary), var(--partner-accent));
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.partner-hero-subtitle {
  font-size: 20px;
  color: var(--partner-text-secondary);
  line-height: 1.6;
  margin: 0 0 40px 0;
}

.partner-hero-cta {
  display: flex;
  gap: 16px;
  justify-content: center;
  flex-wrap: wrap;
}

.partner-cta-primary,
.partner-cta-secondary {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 18px 40px;
  font-size: 17px;
  font-weight: 600;
  text-decoration: none;
  border-radius: 12px;
  transition: var(--partner-transition);
  gap: 8px;
}

.partner-cta-primary {
  background: var(--partner-primary);
  color: white;
  box-shadow: 0 8px 24px rgba(0, 154, 97, 0.25);
}

.partner-cta-primary:hover {
  background: var(--partner-primary-hover);
  transform: translateY(-2px);
  box-shadow: var(--partner-shadow-lg);
}

.partner-cta-secondary {
  background: transparent;
  color: var(--partner-primary);
  border: 2px solid var(--partner-primary);
}

.partner-cta-secondary:hover {
  background: rgba(0, 154, 97, 0.1);
  transform: translateY(-2px);
}

/* Commission Tiers - GLASSMORPHISM */
.partner-tiers {
  padding: 80px 0;
  background: rgba(255, 255, 255, 0.7);
  backdrop-filter: blur(20px) saturate(180%);
  -webkit-backdrop-filter: blur(20px) saturate(180%);
  border-top: 1px solid rgba(255, 255, 255, 0.5);
  border-bottom: 1px solid rgba(255, 255, 255, 0.3);
}

[data-theme="dark"] .partner-tiers {
  background: rgba(10, 10, 10, 0.6);
  backdrop-filter: blur(20px) saturate(180%);
  -webkit-backdrop-filter: blur(20px) saturate(180%);
  border-top: 1px solid rgba(255, 255, 255, 0.1);
  border-bottom: 1px solid rgba(255, 255, 255, 0.05);
}

.partner-tiers-header {
  text-align: center;
  margin-bottom: 60px;
}

.partner-tiers-header h2 {
  font-size: clamp(32px, 4vw, 48px);
  font-weight: 700;
  color: var(--partner-text);
  margin: 0 0 16px 0;
}

.partner-tiers-header p {
  font-size: 18px;
  color: var(--partner-text-secondary);
}

.partner-tiers-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 32px;
}

.partner-tier {
  background: var(--partner-card-bg);
  border: 2px solid var(--partner-border);
  border-radius: 24px;
  padding: 40px 32px;
  text-align: center;
  transition: var(--partner-transition);
  animation: fadeInUp 0.8s ease both;
  animation-delay: calc(var(--tier-index) * 0.1s);
  position: relative;
}

.partner-tier:nth-child(1) { --tier-index: 0; }
.partner-tier:nth-child(2) { --tier-index: 1; }
.partner-tier:nth-child(3) { --tier-index: 2; }

[data-theme="dark"] .partner-tier {
  background: var(--partner-card-bg);
  border-color: rgba(255, 255, 255, 0.08);
  backdrop-filter: blur(10px);
}

.partner-tier.featured {
  border-color: var(--partner-primary);
  transform: scale(1.05);
  box-shadow: var(--partner-shadow-lg);
}

.partner-tier:hover {
  transform: translateY(-4px);
  border-color: var(--partner-primary);
  box-shadow: var(--partner-shadow-lg);
}

.partner-tier.featured:hover {
  transform: scale(1.05) translateY(-4px);
}

.partner-tier-badge {
  position: absolute;
  top: -12px;
  left: 50%;
  transform: translateX(-50%);
  background: linear-gradient(135deg, #ff7e50, #ff9a76);
  color: white;
  padding: 6px 16px;
  border-radius: 100px;
  font-size: 12px;
  font-weight: 700;
  text-transform: uppercase;
  box-shadow: 0 4px 12px rgba(255, 126, 80, 0.4);
}

.partner-tier-name {
  font-size: 24px;
  font-weight: 700;
  color: var(--partner-text);
  margin: 0 0 16px 0;
}

.partner-tier-commission {
  font-size: 56px;
  font-weight: 800;
  background: linear-gradient(135deg, var(--partner-primary), var(--partner-accent));
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
  margin: 0 0 8px 0;
  line-height: 1;
}

.partner-tier-subtitle {
  font-size: 14px;
  color: var(--partner-text-secondary);
  margin: 0 0 32px 0;
}

.partner-tier-features {
  list-style: none;
  padding: 0;
  margin: 0 0 32px 0;
  text-align: left;
}

.partner-tier-features li {
  padding: 12px 0;
  font-size: 15px;
  color: var(--partner-text);
  display: flex;
  align-items: flex-start;
  gap: 12px;
}

.partner-tier-features svg {
  width: 20px;
  height: 20px;
  color: var(--partner-primary);
  flex-shrink: 0;
  margin-top: 2px;
}

/* Benefits Section */
.partner-benefits {
  padding: 100px 0;
  background: var(--partner-bg);
}

.partner-benefits-header {
  text-align: center;
  max-width: 700px;
  margin: 0 auto 80px;
}

.partner-benefits-header h2 {
  font-size: clamp(32px, 4vw, 48px);
  font-weight: 700;
  color: var(--partner-text);
  margin: 0 0 20px 0;
}

.partner-benefits-header p {
  font-size: 18px;
  color: var(--partner-text-secondary);
}

.partner-benefits-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 40px;
}

.partner-benefit {
  text-align: center;
  animation: fadeInUp 0.8s ease both;
  animation-delay: calc(var(--benefit-index) * 0.1s);
}

.partner-benefit:nth-child(1) { --benefit-index: 0; }
.partner-benefit:nth-child(2) { --benefit-index: 1; }
.partner-benefit:nth-child(3) { --benefit-index: 2; }
.partner-benefit:nth-child(4) { --benefit-index: 3; }
.partner-benefit:nth-child(5) { --benefit-index: 4; }
.partner-benefit:nth-child(6) { --benefit-index: 5; }

.partner-benefit-icon {
  width: 80px;
  height: 80px;
  background: linear-gradient(135deg, var(--partner-primary), var(--partner-accent));
  border-radius: 20px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto 24px;
  box-shadow: 0 8px 20px rgba(0, 154, 97, 0.25);
}

[data-theme="dark"] .partner-benefit-icon {
  background: linear-gradient(135deg, rgba(0, 217, 126, 0.2), rgba(0, 154, 97, 0.2));
  box-shadow: 0 8px 20px rgba(0, 217, 126, 0.2);
}

.partner-benefit-icon svg {
  width: 40px;
  height: 40px;
  color: white;
  stroke-width: 2;
}

[data-theme="dark"] .partner-benefit-icon svg {
  color: var(--partner-primary);
}

.partner-benefit h3 {
  font-size: 20px;
  font-weight: 700;
  color: var(--partner-text);
  margin: 0 0 12px 0;
}

.partner-benefit p {
  font-size: 15px;
  color: var(--partner-text-secondary);
  line-height: 1.6;
  margin: 0;
}

/* Final CTA */
.partner-final-cta {
  padding: 100px 0;
  background: linear-gradient(135deg, #f0faf7 0%, #ffffff 100%);
  text-align: center;
}

[data-theme="dark"] .partner-final-cta {
  background: linear-gradient(135deg, rgba(0, 217, 126, 0.05) 0%, transparent 100%);
}

.partner-final-cta h2 {
  font-size: clamp(32px, 4vw, 48px);
  font-weight: 700;
  color: var(--partner-text);
  margin: 0 0 24px 0;
}

.partner-final-cta p {
  font-size: 18px;
  color: var(--partner-text-secondary);
  margin: 0 0 40px 0;
  max-width: 600px;
  margin-left: auto;
  margin-right: auto;
}

/* Responsive - Tablet */
@media (max-width: 1024px) {
  .partner-container {
    padding: 0 32px;
  }
  
  .partner-tiers-grid {
    grid-template-columns: 1fr;
    gap: 24px;
  }
  
  .partner-tier.featured {
    transform: scale(1);
  }
  
  .partner-benefits-grid {
    grid-template-columns: repeat(2, 1fr);
    gap: 32px;
  }
}

/* Responsive - Mobile */
@media (max-width: 768px) {
  .partner-hero {
    padding: 80px 0 60px 0;
  }
  
  .partner-container {
    padding: 0 20px;
  }
  
  .partner-hero-title {
    font-size: 36px;
  }
  
  .partner-hero-subtitle {
    font-size: 17px;
  }
  
  .partner-hero-cta {
    flex-direction: column;
  }
  
  .partner-cta-primary,
  .partner-cta-secondary {
    width: 100%;
  }
  
  .partner-tiers {
    padding: 60px 0;
  }
  
  .partner-tier-commission {
    font-size: 48px;
  }
  
  .partner-benefits {
    padding: 60px 0;
  }
  
  .partner-benefits-grid {
    grid-template-columns: 1fr;
    gap: 40px;
  }
  
  .partner-final-cta {
    padding: 60px 0;
  }
}

/* Animations */
@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(30px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

/* Accessibility */
@media (prefers-reduced-motion: reduce) {
  * {
    animation-duration: 0.01ms !important;
    animation-iteration-count: 1 !important;
    transition-duration: 0.01ms !important;
  }
}
</style>

