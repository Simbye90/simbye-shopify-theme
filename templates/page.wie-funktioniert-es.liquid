<!-- SIMBYE ESIM INSTALLATION GUIDE - ENTERPRISE EDITION -->

<style>
/* CSS Variables */
:root {
  --primary: #009a61;
  --primary-hover: #00824f;
  --accent: #00d97e;
  --text: #1a1a1a;
  --text-secondary: #666666;
  --bg: #ffffff;
  --card-bg: #ffffff;
  --border: rgba(0, 0, 0, 0.08);
  --shadow-sm: 0 4px 12px rgba(0, 0, 0, 0.06);
  --shadow-md: 0 8px 24px rgba(0, 0, 0, 0.08);
  --radius: 24px;
}

/* Dark Mode */
[data-theme="dark"] {
  --text: #f5f5f5;
  --text-secondary: #999999;
  --bg: #0a0a0a;
  --card-bg: rgba(26, 26, 26, 0.8);
  --border: rgba(255, 255, 255, 0.08);
  --primary: #00d97e;
  --accent: #00ff88;
}

/* Kill Shopify Grid */
.shopify-section:has(.installation-section) {
  display: block !important;
  padding: 0 !important;
  margin: 0 !important;
  grid-template-columns: unset !important;
}

/* Main Section */
.installation-section {
  width: 100%;
  padding: 0;
  background: var(--bg);
  position: relative;
  overflow: hidden;
}

/* Container */
.installation-container {
  max-width: 1600px;
  margin: 0 auto;
  padding: 60px 40px 80px 40px;
  width: 100%;
  position: relative;
}

/* Header */
.installation-header {
  text-align: center;
  margin-bottom: 72px;
}

/* H1 Title - SIMPLE */
.installation-title {
  font-size: 56px;
  font-weight: 900;
  line-height: 1.1;
  margin: 0 0 24px 0;
  letter-spacing: -0.02em;
  color: var(--text);
}

.installation-title-line1 {
  display: block;
  color: var(--text);
  margin-bottom: 6px;
}

.installation-title-highlight {
  display: block;
  background: linear-gradient(135deg, var(--primary), var(--accent));
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

/* Subtitle */
.installation-subtitle {
  font-size: 19px;
  color: var(--text-secondary);
  margin: 0;
  font-weight: 500;
  line-height: 1.5;
}

/* Device Tabs */
.device-tabs {
  display: flex;
  gap: 4px;
  background: rgba(0, 154, 97, 0.05);
  padding: 4px;
  border-radius: 16px;
  border: 1px solid rgba(0, 154, 97, 0.1);
  margin-bottom: 48px;
  max-width: 600px;
  margin-left: auto;
  margin-right: auto;
  box-shadow: var(--shadow-sm);
}

.device-tab {
  flex: 1;
  padding: 16px 32px;
  background: transparent;
  border: none;
  border-radius: 12px;
  font-size: 18px;
  font-weight: 600;
  color: var(--text-secondary);
  cursor: pointer;
  transition: all 0.3s ease;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 12px;
  font-family: inherit;
}

.device-tab:hover {
  background: rgba(0, 154, 97, 0.08);
  color: var(--text);
}

.device-tab.active {
  background: var(--primary);
  color: white;
  box-shadow: 0 4px 12px rgba(0, 154, 97, 0.3);
}

.tab-icon {
  width: 24px;
  height: 24px;
}

/* Tab Content */
.tab-content {
  display: none;
}

.tab-content.active {
  display: block;
}

/* Steps List */
.steps-list {
  display: flex;
  flex-direction: column;
  gap: 32px;
}

/* Step Card - KEINE HOVER ANIMATION */
.step-card {
  background: var(--card-bg);
  border-radius: var(--radius);
  overflow: hidden;
  box-shadow: var(--shadow-md);
  position: relative;
  border: 2px solid var(--border);
}

.step-layout {
  display: grid;
  grid-template-columns: 500px 1fr;
  min-height: 320px;
}

/* Step Image */
.step-image-wrapper {
  position: relative;
  background: linear-gradient(135deg, #f8fffe 0%, #ffffff 100%);
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
}

.step-image {
  width: 100%;
  height: 100%;
  object-fit: contain;
  padding: 20px;
}

/* Step Badge */
.step-badge {
  position: absolute;
  top: 20px;
  left: 20px;
  width: 48px;
  height: 48px;
  background: linear-gradient(135deg, var(--primary), var(--accent));
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-size: 20px;
  font-weight: 700;
  box-shadow: 0 8px 20px rgba(0, 154, 97, 0.3);
  z-index: 2;
}

/* Step Content */
.step-content {
  padding: 40px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.step-title {
  font-size: 24px;
  font-weight: 700;
  color: var(--text);
  margin: 0 0 16px 0;
  letter-spacing: -0.01em;
}

.step-instructions {
  list-style: none;
  padding: 0;
  margin: 0 0 24px 0;
}

.step-instructions li {
  font-size: 16px;
  color: var(--text-secondary);
  line-height: 1.6;
  margin-bottom: 12px;
  padding-left: 28px;
  position: relative;
}

.step-instructions li::before {
  content: '→';
  position: absolute;
  left: 0;
  color: var(--primary);
  font-weight: 700;
}

/* Note Box */
.step-note {
  background: linear-gradient(135deg, #f8fffe 0%, #ffffff 100%);
  border-left: 4px solid var(--primary);
  border-radius: 12px;
  padding: 16px;
  margin-top: auto;
}

.step-note-title {
  font-size: 13px;
  font-weight: 700;
  color: var(--primary);
  text-transform: uppercase;
  margin: 0 0 6px 0;
  letter-spacing: 0.05em;
}

.step-note-text {
  font-size: 14px;
  color: var(--text-secondary);
  line-height: 1.5;
  margin: 0;
}

/* APN Settings Card */
.apn-settings-card {
  background: linear-gradient(135deg, rgba(255, 126, 80, 0.05), rgba(255, 154, 118, 0.05));
  border: 2px solid #ff7e50;
  margin-top: 32px;
}

.apn-settings-card .step-layout {
  grid-template-columns: 1fr;
}

.apn-settings-card .step-content {
  padding: 48px;
}

.apn-icon-badge {
  width: 64px;
  height: 64px;
  background: linear-gradient(135deg, #ff7e50, #ff9a76);
  border-radius: 16px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 24px;
  box-shadow: 0 8px 20px rgba(255, 126, 80, 0.25);
}

.apn-icon-badge svg {
  width: 32px;
  height: 32px;
  color: white;
}

/* CTA Section */
.installation-cta {
  text-align: center;
  margin-top: 80px;
  padding: 48px;
  background: linear-gradient(135deg, #f8fffe 0%, #ffffff 100%);
  border-radius: var(--radius);
  border: 2px solid var(--border);
}

.cta-title {
  font-size: 36px;
  font-weight: 700;
  color: var(--text);
  margin: 0 0 16px 0;
}

.cta-subtitle {
  font-size: 18px;
  color: var(--text-secondary);
  margin: 0 0 32px 0;
}

.cta-button {
  display: inline-block;
  padding: 20px 48px;
  background: linear-gradient(135deg, var(--primary), var(--accent));
  color: white;
  border: none;
  border-radius: 100px;
  font-size: 18px;
  font-weight: 700;
  cursor: pointer;
  text-decoration: none;
  transition: transform 0.3s ease, box-shadow 0.3s ease;
  box-shadow: 0 8px 24px rgba(0, 154, 97, 0.25);
}

.cta-button:hover {
  transform: translateY(-2px);
  box-shadow: 0 12px 32px rgba(0, 154, 97, 0.35);
}

.cta-button svg {
  display: inline-block;
  vertical-align: middle;
  margin-right: 8px;
}

/* MOBILE STYLES */
@media (max-width: 768px) {
  /* Container */
  .installation-container {
    padding: 30px 20px 0px 20px;
  }
  
  .installation-header {
    margin-bottom: 48px;
  }
  
  /* H1 MOBILE: 32px */
  .installation-title {
    font-size: 32px;
    margin-bottom: 20px;
  }
  
  .installation-subtitle {
    font-size: 16px;
  }
  
  /* Tabs */
  .device-tabs {
    margin-bottom: 32px;
  }
  
  .device-tab {
    padding: 12px 20px;
    font-size: 16px;
  }
  
  .tab-icon {
    width: 20px;
    height: 20px;
  }
  
  /* Step Cards */
  .step-layout {
    grid-template-columns: 1fr;
  }
  
  .step-image-wrapper {
    height: 240px;
  }
  
  .step-content {
    padding: 24px;
  }
  
  .step-title {
    font-size: 20px;
  }
  
  /* APN Card */
  .apn-settings-card .step-content {
    padding: 32px 24px;
  }
  
  .apn-icon-badge {
    width: 48px;
    height: 48px;
    margin-bottom: 20px;
  }
  
  .apn-icon-badge svg {
    width: 24px;
    height: 24px;
  }
  
  /* CTA */
  .installation-cta {
    padding: 32px 24px;
  }
  
  .cta-title {
    font-size: 28px;
  }
  
  .cta-button {
    padding: 16px 32px;
  }
}
</style>

